<div class='background statsbg'></div>
<h2>Hero Stats!</h2>

<table class="striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Health</th>
            <th>Attack</th>
            <th>Defence</th>
        </tr>
    </thead>

    <tbody>
        {{#each characters}}
        <tr>
            <td>{{name}}</td>
            <td>{{attack}}</td>
            <td>{{defense}}</td>
            <td>{{health}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>